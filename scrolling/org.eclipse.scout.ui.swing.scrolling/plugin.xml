<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <extension id="app" name="Application" point="org.eclipse.core.runtime.applications">
    <application>
      <run class="org.eclipse.scout.ui.swing.scrolling.SwingApplication"/>
    </application>
  </extension>
  <extension id="product" point="org.eclipse.core.runtime.products">
    <product
          application="org.eclipse.scout.ui.swing.scrolling.app"
          name="scoutScrolling">
       <property
             name="appName"
             value="scoutScrolling">
       </property>
    </product>
  </extension>
</plugin>
